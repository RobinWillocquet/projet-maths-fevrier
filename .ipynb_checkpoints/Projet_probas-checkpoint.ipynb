    "def tauxrejet(n):\n",
    "    compteur = 0\n",
    "    for i in range(n):\n",
    "        x = np.random.normal(0,1,np.shape(unknown_indexes)[0])\n",
    "        simu = Ec + np.matmul(Cholesky,x)\n",
